{
  "name": "access-map0",
  "version": "1.0.0",
  "description": "A community-driven accessibility mapping platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm start",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "accessibility",
    "mapping",
    "community",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "Ankit D. Vishwakarma, Amisth Mahendrakar",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}